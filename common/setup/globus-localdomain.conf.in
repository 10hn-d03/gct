# This file contains the default domain name for the site.
# The value for the environment variable, "GLOBUS_LOCALDOMAIN",
# was specificed with the --with-domain-name=DOMAIN 
# during globus-install

if [ -z "${LOCALDOMAIN}" ] ; then 
  GLOBUS_LOCALDOMAIN="@LOCALDOMAIN@"
else
  GLOBUS_LOCALDOMAIN="${LOCALDOMAIN}"
fi
export GLOBUS_LOCALDOMAIN
