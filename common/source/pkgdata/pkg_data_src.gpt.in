<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE gpt_package_metadata SYSTEM "package.dtd">

<gpt_package_metadata Name="@PACKAGE@" Version="@VERSION@" Format_Version="0.01">
<src_pkg>

<With_Flavors build="yes" />
<Build_Environment>
<cflags>@GPT_CFLAGS@</cflags>
<external_includes>@GPT_INCLUDES@</external_includes>
<external_libs>@GPT_LIBS@</external_libs>
<pkg_libs>-lglobus_common_$GLOBUS_FLAVOR_NAME</pkg_libs>
</Build_Environment>

</src_pkg>
</gpt_package_metadata>

