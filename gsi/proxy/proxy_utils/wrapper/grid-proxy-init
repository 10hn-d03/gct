#!/bin/sh

if test "X${GT_PROXY_MODE}" = "Xold" ; then
    if echo $@ | grep new >/dev/null 2>&1 ; then
        ARGS=`echo $@ | sed s/\ -new\ // | sed s/^-new//`
        grid-proxy-init-bin $ARGS
    else
        grid-proxy-init-bin -old $@
    fi
else
    ARGS=`echo $@ | sed s/\ -new\ // | sed s/^-new//`
    grid-proxy-init-bin $ARGS
fi

exit $?
